<h2>New Ticket</h2>
<%= form_for(@ticket) do |f| %>
  <p>
    <%= f.label :name %> <br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>

  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <%= f.submit %>
  <% end %>
